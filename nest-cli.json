{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "generateOptions": {
    "spec": false,
    "flat": false
  },
  "monorepoMode": false,
  "compilerOptions": {
    "builder": {
      "type": "swc",
      "options": {
        "extensions": [".ts", ".js", ".tsx", ".jsx"]
      }
    }
  },
  "projects": {
    "application": {
      "root": "src/application",
      "sourceRoot": "src/application",
      "generateOptions": {
        "spec": false,
        "flat": false
      }
    },
    "domain": {
      "root": "src/domain",
      "sourceRoot": "src/domain",
      "generateOptions": {
        "spec": false,
        "flat": false
      }
    },
    "infrastructure": {
      "root": "src/infrastructure",
      "sourceRoot": "src/infrastructure",
      "generateOptions": {
        "spec": false,
        "flat": false
      }
    },
    "modules": {
      "root": "src/modules",
      "sourceRoot": "src/modules",
      "generateOptions": {
        "spec": false,
        "flat": false
      }
    },
    "shared": {
      "root": "src/shared",
      "sourceRoot": "src/shared",
      "generateOptions": {
        "spec": false,
        "flat": false
      }
    }
  }
}